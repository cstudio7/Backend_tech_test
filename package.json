{
  "name": "fig_financetest",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/cstudio7/fig_financetest.git",
  "author": "cstudio7 <conquerorsword36@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "babel src --out-dir production",
    "start": "npm run build && NODE_ENV=production node ./production/index.js",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register src/tests/*.js  --require @babel/polyfill --require @babel/register --timeout 100000000 --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "db-seed": "babel-node ./src/models/seed/seedEvents.js"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "body-parser": "^1.19.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "cors": "^2.8.5",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "mocha": "^9.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.2.7",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4"
  }
}
